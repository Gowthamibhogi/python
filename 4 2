n=int(input())
m=[]
z=[]
for i in range(n):
    l=[]
    for j in range(n):
        x=int(input())
        l.append(x)
    m.append(l)
for i in range(n):
    for j in range(n):
        if i==j:
            z.append(m[i][j])
        if i+j==n-1:
            z.append(m[i][j])
for i in z:
    print(i,end=' ')
print("Max inÂ list:",max(z))
